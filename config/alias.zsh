# Alias Definitions

export EDITOR="vim"
alias ls='ls --color=auto'
alias ll='ls -l'
alias la='ls -a'
alias lh='ll -h'
alias lla='ll -a'
alias lha='lh -a'
alias vi='vim'
alias grep="grep --color=auto"
alias less="less -R"

#global aliases

alias -g L="| less"
alias -g ">q"="2>&1 >/dev/null"
alias -g ">e"="2>/dev/null"

# END Alias
